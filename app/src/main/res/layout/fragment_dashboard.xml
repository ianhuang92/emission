<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="monash.emission.account.DashboardFragment">


    <TextView
        android:text="TextView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="39dp"
        android:id="@+id/welcomeText"
        android:textAppearance="@style/TextAppearance.AppCompat.Headline"
        android:textAlignment="center" />

    <TextView
        android:text="My Emission Data"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="16dp"
        android:id="@+id/textView18"
        android:layout_below="@+id/reportBTN"
        android:layout_alignEnd="@+id/reportBTN" />

    <TextView
        android:text="My Emission Ranking"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/textView19"
        android:layout_below="@+id/reportBTN"
        android:layout_alignEnd="@+id/rankBTN" />

    <Button
        android:text="Place Holder"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/button4"
        android:layout_marginTop="35dp"
        android:layout_below="@+id/textView19"
        android:layout_alignEnd="@+id/textView19"
        android:textAllCaps="false" />

    <ImageButton
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:srcCompat="@drawable/rank"
        android:id="@+id/rankBTN"
        android:layout_marginEnd="16dp"
        android:scaleType="fitCenter"
        android:layout_marginLeft="12dp"
        android:layout_above="@+id/textView19"
        android:layout_alignParentEnd="true" />

    <Button
        android:text="Place Holder"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:id="@+id/button2"
        android:textAllCaps="false"
        android:layout_alignTop="@+id/button4"
        android:layout_alignStart="@+id/reportBTN" />

    <ImageButton
        android:layout_height="150dp"
        app:srcCompat="@drawable/report"
        android:layout_marginTop="61dp"
        android:id="@+id/reportBTN"
        android:layout_marginStart="25dp"
        android:layout_width="150dp"
        android:layout_below="@+id/welcomeText"
        android:layout_alignParentStart="true" />
</RelativeLayout>
