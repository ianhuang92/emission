<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="monash.emission">

    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/start"
        android:label="Emission Tracker"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".IndustrySectorActivity" />
        <activity android:name=".CoffeeRoastingFuncV2.CoffeeRoastActivity"/>
        <activity
            android:name=".CalculationActivity"
            android:label="@string/title_activity_calculation"
            android:theme="@style/AppTheme"
            ></activity>
    </application>

</manifest>